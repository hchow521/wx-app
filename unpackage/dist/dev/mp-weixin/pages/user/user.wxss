.select-btn {
  height: 50rpx;
  line-height: 50rpx;
  border: 1px solid #e3e4e6;
  border-radius: 10rpx;
  color: #979899;
  font-size: 24rpx;
  padding: 0 57rpx 0 24rpx;
  position: relative;
}
.select-btn::after {
  content: '';
  position: absolute;
  right: 17rpx;
  top: 2rpx;
  display: inline-block;
  width: 21rpx;
  height: 50rpx;
  background: url('data:image/png;base64,') no-repeat center center / 21rpx auto;
}
.select-btn:active {
  border-color: #999;
}
.logo-img {
  width: 183rpx;
  height: 38rpx;
  position: absolute;
  top: 33rpx;
  left: 27rpx;
}
.like-wrap {
  width: 696rpx;
  margin: 27rpx 0 0 27rpx;
  max-height: calc(100% - 54rpx);
  background: #fffeff;
  box-shadow: 0px 6rpx 30rpx 0px #e6e6e6;
  border-radius: 10rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  position: relative;
  z-index: 1;
}
.like-wrap .like-list {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  overflow-y: auto;
  padding: 0 28rpx;
  box-sizing: border-box;
}
.like-wrap .like-list .like-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 148rpx;
  border-bottom: 1px solid #F0F0F0;
}
.like-wrap .like-list .like-item:last-child {
  border: 0;
}
.like-wrap .like-list .like-item .like-icon {
  -webkit-box-flex: 0;
  -webkit-flex: 0 0 48rpx;
          flex: 0 0 48rpx;
  width: 48rpx;
  height: 48rpx;
}
.like-wrap .like-list .like-item .like-title {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  color: #3A4044;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 0 29rpx;
  font-size: 28rpx;
}
.like-wrap .like-list .like-item .like-time {
  color: #C0C1C2;
  font-size: 26rpx;
}
.like-wrap .other-status-wrap {
  height: 495rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.like-wrap .other-status-wrap > text {
  font-size: 30rpx;
  color: #B3BBC7;
}
.like-wrap .other-status-wrap .box-img {
  width: 238rpx;
  height: 179rpx;
  margin-bottom: 27rpx;
}
.like-wrap .other-status-wrap .search-img {
  width: 396rpx;
  height: 226rpx;
  margin-bottom: 13rpx;
}
.full-column {
  overflow: hidden;
}
.user-wrap {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 98rpx 66rpx 30rpx;
  box-sizing: border-box;
  position: relative;
  z-index: 1;
  /* 按钮改文本样式 */
}
.user-wrap .user-avatar {
  -webkit-box-flex: 0;
  -webkit-flex: 0 0 136rpx;
          flex: 0 0 136rpx;
  width: 136rpx;
  height: 136rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 39rpx;
}
.user-wrap .user-avatar.not-login {
  background-color: #FFFEFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.user-wrap .user-avatar.not-login > image {
  width: 62rpx;
  height: 60rpx;
}
.user-wrap .text-btn,
.user-wrap .text-view {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  background-color: transparent;
  text-align: left;
  color: #fff;
  font-size: 28rpx;
  padding: 0;
}
.user-wrap button.text-btn,
.user-wrap button.text-btn:active,
.user-wrap button.text-btn:focus {
  background-color: transparent;
  color: #fff;
}
.user-wrap button.text-btn::after {
  content: '';
  border: 0;
}
.content {
  width: 100%;
  height: 100vh;
  position: relative;
}
.content .user-bgi {
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
}

